/ - - - - - - - - - - - - - Header - - - - - - - - - - - - - - - -
%header#header.header.style-2.sticky-header
  / - - - - - - - - - - - / Mobile Menu - - - - - - - - - - - - -
  / main menu
  .menu-holder
    .container
      .menu-wrap
        .nav-item
          / - - - - - - - - - - - - - Navigation - - - - - - - - - - - - - - - -
          %nav#main-navigation.main-navigation
            %ul#menu.clearfix
              %li.current
                %a{:href => "#"} Главная
              %li.dropdown.has-megamenu
                %a{:href => "#"} Услуги
                / sub menu
                .sub-menu-wrap.mega-menu.flex-row
                  -Category.all.each do |category|
                    %ul
                      -category.subcategories.each do |subcat|
                        %li
                          %a{:href => category_subcategory_path(category, subcat)}=subcat.title
              %li.dropdown
                %a{:href => "#"} Компания
                / sub menu
                .sub-menu-wrap
                  %ul
                    %li
                      %a{:href => "attorneys_list_v1.html"} Реализованные кейсы
                    %li
                      %a{:href => "attorneys_list_v2.html"} Статьи
                    %li
                      %a{:href => "attorney_profile_v1.html"} Блоги
              %li
                %a{:href => "#"} Контакты
                / sub menu
          / - - - - - - - - - - - - end Navigation - - - - - - - - - - - - - - -
  .top-header
    .container
      .flex-row.flex-center.flex-justify
        .col
          / Contact info
          %ul.contact-info.style-2
            %li.info-item
              .pre Звоните ещё сегодня
              .item-info
                %span{:content => "telephone=no"}=link_to Setting.try(:first).try(:phone), "tel:#{Setting.try(:first).try(:phone)}"
        .col
          / logo
          .logo-wrap
            %a.logo{:href => "index.html"}
              %img{:alt => "", :src => "#{asset_path 'custom/logo.png'}"}/
        .col
          %a.btn.btn-big.btn-style-5{:href => "#"} Получить консультацию
/ - - - - - - - - - - - - end Header - - - - - - - - - - - - - - -
